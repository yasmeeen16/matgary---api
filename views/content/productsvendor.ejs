    <style >
    img {
          border: 1px solid #ddd;
          border-radius: 4px;
          padding: 5px;
          width: 150px!important;
          height: 150px;
          border-radius: 50%;
        }

      #add:link, #add:visited {
              background-color: #f44336;
              color: white;
              padding: 14px 25px;
              text-align: center;
              text-decoration: none;
              display: inline-block;
              }

              a:hover, a:active {
              background-color: red;
              }

