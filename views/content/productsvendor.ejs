<body>
  <div class="wrapper">
      <!-- Navigation -->
      <nav class="navbar navbar-inverse navbar-fixed-top" role="navigation">
          <!-- Brand and toggle get grouped for better mobile display -->
          <div class="navbar-header" style="float: right">
              <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-ex1-collapse">
                  <span class="sr-only">Toggle navigation</span>
                  <span class="icon-bar"></span>
                  <span class="icon-bar"></span>
                  <span class="icon-bar"></span>
              </button>
              <a class="navbar-brand" href="#">
                  <h1 class="text-center">متجري</h1>
              </a>
          </div>
          <!-- Top Menu Items -->




          <ul class="nav navbar-left top-nav">
              <li>
                  <a href="/webvendor/addproductToCat" class="btn btn-default">
                    اضافه منتج
                  </a>
              </li>

          </ul>
          <!-- Sidebar Menu Items - These collapse to the responsive navigation menu on small screens -->
          <div class="collapse navbar-collapse navbar-ex1-collapse">
              <ul class="nav navbar-nav side-nav">




                  <li>
                      <a href="/webvendor/productvendor">

                          منتجاتى

                          <i class="fa fa-money" aria-hidden="true"></i>
                      </a>
                  </li>
                  <li>
                  <a href="/webvendor/logout">

                          تسجيل خروج
                          <i class="fa fa-money" aria-hidden="true"></i>
                      </a>
                  </li>

              </ul>
          </div>
          <!-- /.navbar-collapse -->
      </nav>

<div class="main-panel">
      <div class="content">
          <div class="container-fluid">
              <div class="row">
                  <div class="col-md-12">
                      <div class="card">

                          <div class="content table-responsive table-full-width">
                              <table class="table">
                                  <thead>
                                      <th>المنتج</th>
                                      <th>الوصف</th>
                                      <th>رقم الموديل</th>

                                  </thead>
                                  <tbody>
                                    <%products.forEach(function(product){ %>
                                      <tr>
                                          <td>
                                              <div class="product">
                                                  <div class="product-img"><img src="<%=product.imgs[0]%>"> </div>
                                                  <div class="contain" style="display: flex;flex-direction: column">
                                                      <div class="product-data">
                                                          <a href="#"><%=product.Ename%></a>

                                                          <div>
                                                              <span>Category</span> &nbsp;
                                                              <i class="fa fa-chevron-right"></i>
                                                              <span class="visble">Subcategry</span>
                                                              &nbsp; | &nbsp;
                                                              <span>1000$</span>
                                                          </div>

                                                      </div>

                                                  </div>

                                              </div>
                                          </td>

                                          <td>
                                              <div>
                                                  <p style="margin-bottom: 20px;"><%=product.discriptionEnglish%></p>

                                              </div>

                                          </td>
                                          <td>
                                              <div>
                                                  <p style="margin-bottom: 20px;font-weight: 600;font-size:18px;"><%=product.modelnumber%></p>

                                              </div>

                                          </td>

                                          <td> <a><i class="fa fa-ellipsis-h fa-2x dot"></i></a>
                                              <ul class="little text-right">
                                                  <li><a href="view.html">عرض</a></li>
                                                  <li><a href="edit.html">تعديل</a></li>
                                                  <li><a class="del">حذف</a></li>

                                              </ul>
                                          </td>


                                      </tr>

                                      <%});%>
                                  </tbody>
                              </table>

                          </div>
                      </div>
                  </div>


              </div>
          </div>
      </div>

















    </div>
    <script>
        var x = document.getElementsByClassName('dot');
        for (let a = 0; a < x.length; a++) {
            document.getElementsByClassName('dot')[a].addEventListener('click', function show() {
                document.getElementsByClassName('little')[a].classList.toggle('little-v');
            });
        }

    </script>
