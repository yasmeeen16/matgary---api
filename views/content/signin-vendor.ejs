<body>
    <div class="content">
        <div class="container-fluid">
            <div class="row">
                <div class="col-xs-offset-3 col-xs-6 col-xs-offset-3 form-container">
                    <div class="form-img">
                        <p>تسجيل دخول</p>
                    </div>
                    <div class="form">
                        <form style="width: 50%" id="login-form" action="/wauthvendor/login" method="post" role="form">
                            <input type="email" name="email" id="email" tabindex="1" class="form-control" placeholder="البريد الالكتروني" value="">
                            <input type="password" name="password" id="password" tabindex="2" class="form-control" placeholder="الرقم السري">
                            <div class="add" style="">
                                <button type="submit">تسجيل دخول</button>
                            </div>
                        </form>
                        <div style="padding: 30px; font-size: 17px;">
                            <span>مستخدم جديد ؟</span>&nbsp;
                            <a href="/wauthvendor/register">تسجيل</a>

                        </div>

                    </div>

                </div>

            </div>
        </div>

    </div>

</body>


