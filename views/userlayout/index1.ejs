	<div id="carouselExampleIndicators" class="carousel slide" data-ride="carousel">
		<ol class="carousel-indicators">
			<li data-target="#carouselExampleIndicators" data-slide-to="0" class="active"></li>
			<li data-target="#carouselExampleIndicators" data-slide-to="1"></li>
			<li data-target="#carouselExampleIndicators" data-slide-to="2"></li>
		</ol>
		<div class="carousel-inner">
			<div class="carousel-item active">
				<img class="d-block w-100" src="<%=offers[0].img%>" alt="First slide">
			</div>
			<%offers.forEach(function(offer){ %>
			<div class="carousel-item">
				<img class="d-block w-100" src="<%=offer.img%>" alt="Second slide">
			</div>
			<%});%>

		</div>
		<a class="carousel-control-prev" href="#carouselExampleIndicators" role="button" data-slide="prev">
			<span class="icon-e-02 t-l" aria-hidden="true"></span>
			<span class="sr-only">Previous</span>
		</a>
		<a class="carousel-control-next" href="#carouselExampleIndicators" role="button" data-slide="next">
			<span class="icon-e-03 t-l" aria-hidden="true"></span>
			<span class="sr-only">Next</span>
		</a>
	</div>
	<div id="tt-pageContent">
	<div class="addons">
		<img src="/user-static/images/gif/add-1.gif" alt="">
	</div>
<%offers.forEach(function(offer){ console.log(typeof(offer.catsID)) %>

	<div class="">
		<div   class="addons col-xs-12">
			<img style="width:100% !important ;height:200px" src="<%=offer.img%>" alt="">
		</div>
	<div class="itemsSlider">
		<div class="sliderTitle">
			<h4><%=offer.name%></h4>
			<a href="" class="Viewall">
				view all
			</a>
		</div>
		<!-- <script type="text/javascript">
		mydelete();
		</script> -->
		<div class="ProSlider items product">
			<%offer.catsID.forEach(function(catsID){ %>
					<%catsID.productsID.forEach(function(product){ %>
										<div class="item">
	    		<a class="" href="">
	    			<div class="proimage">
	    				<div class="">
    						<img src="<%=product.imgs[0]%>" alt="">
	    				</div>
	    				<div class="">
    						<img src="/user-static/images/p/now.svg" alt="" class="nowlable">
		    			</div>
	    				<div class="actionContainer">
	    					<button aria-label="Add To wishlist" class="actionBtn addtocardbtn"  onclick="addTowishlist('<%=userId%>','<%=product._id%>')">
	    						<i class="icon-h-13"></i>
	    					</button>
		    			</div>
		    		</div>
		    		<div class="prodetails">
	    				<p class="brand">
		    				<%=product.brandArabic%>
		    			</p>
	    				<div class="name" style="overflow: hidden;">
							<p>
								<%=product.Aname%>
							</p>
	    				</div>
	    				<div class="priceRow">
	    					<div class="productPriceContainer">
	    						<p class="proPrice">
    								<span class="preReductionPrice">
	    								<%=product.price1%>
	    							</span>
	    							<span class="discount">
		    							<span class="discountBadge">
		    							<%=product.discount%> off
		    							</span>
	    							</span>
	    							<span class="sellingPrice">
		    							<%=product.price2%>
		    						</span>
		    					</p>
		    				</div>
		    				<div class="actionContainer">
		    					<button type="button" aria-label="Add To Cart" class="actionBtn addtocardbtn" onclick="addTocard('<%=userId%>','<%=product._id%>')">
		    						<i class="icon-g-48" ></i>
		    					</button>
		    				</div>
		    			</div>
		    		</div>
		    	</a>
		    </div>


					<%});%>

			<%});%>


	</div>
</div>
<%});%>
